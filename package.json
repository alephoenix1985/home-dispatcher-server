{
  "name": "psf-aws-mongo-event-driven-service",
  "version": "1.0.0",
  "description": "A PSF service that interacts with MongoDB based on AWS SQS/SNS events.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "npm run start -- dev",
    "start:prod": "npm run start -- prod",
    "dev": "nodemon src/index.js dev",
    "db:seed": "node src/migrations/auth.seed.js",
    "db:seed:dev": "npm run db:seed -- dev",
    "db:seed:prod": "npm run db:seed -- prod",
    "compose:generate": "node scripts/generate-compose.js",
    "compose:generate:prod": "npm run compose:generate -- prod",
    "compose:generate:dev": "npm run compose:generate -- dev",
    "wt-config:generate": "node scripts/generate-watchtower-config.js",
    "wt-config:generate:prod": "npm run wt-config:generate -- prod",
    "wt-config:generate:dev": "npm run wt-config:generate -- dev"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.18.0",
    "psf-core": "file:./core",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.2"
  }
}
