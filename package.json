{
  "name": "psf-db",
  "version": "1.0.0",
  "description": "A PSF service that interacts with MongoDB based on AWS SQS/SNS events.",
  "type": "module",
  "workspaces": [
    "core"
  ],
  "scripts": {
    "db:seed": "node scripts/run-migrations.js",
    "db:seed:dev": "npm run db:seed -- dev",
    "db:seed:prod": "npm run db:seed -- prod",
    "compose-db:generate": "node core/scripts/generate-compose.js",
    "compose-db:generate:prod": "npm run compose-db:generate -- prod",
    "compose-db:generate:dev": "npm run compose-db:generate -- dev",
    "dev": "npm run dev:dev",
    "dev:dev": "dotenv -e .env.dev -- sst dev --stage dev",
    "dev:prod": "dotenv -e .env.prod -- sst dev --stage prod --verbose",
    "deploy:dev": "dotenv -e .env.dev -- sst deploy --stage dev",
    "deploy:prod": "dotenv -e .env.prod -- sst deploy --stage prod --verbose",
    "remove:dev": "dotenv -e .env.dev -- sst remove --stage dev",
    "remove:prod": "dotenv -e .env.prod -- sst remove --stage prod"
  },
  "dependencies": {
    "@mongodb-js/saslprep": "^1.3.0",
    "bson": "^6.10.4",
    "mongodb": "^6.19.0",
    "mongodb-connection-string-url": "^3.0.2",
    "psf-core-node": "file:./core/node",
    "psf-core-shared": "file:./core/shared",
    "webidl-conversions": "^7.0.0",
    "whatwg-url": "^14.2.0"
  }
}