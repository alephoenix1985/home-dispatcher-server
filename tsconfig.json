{
  "extends": "./core/shared/configs/tsconfig/tsconfig-next.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "psf-core-shared/*": [
        "core/shared/*"
      ],
      "psf-core-next/*": [
        "core/next/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "sst-env.d.ts",
    "src/**/*",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
