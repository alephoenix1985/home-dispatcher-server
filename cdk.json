{
  "app": "node cdk/bin/cdk-launcher.js",
  "watch": {
    "include": [
      "cdk/**/*.js",
      "core/**/*.js",
      "src/**/*.js",
      "package.json"
    ],
    "exclude": [
      "README.md",
      "cdk.out/**",
      "node_modules/**"
    ]
  },
  "context": {
    "@aws-cdk/core:newStyleStackSynthesis": true
  }
}
