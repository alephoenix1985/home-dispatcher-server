{
  "name": "psf-db",
  "version": "1.0.0",
  "description": "A PSF service that interacts with MongoDB based on AWS SQS/SNS events.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "npm run start -- dev",
    "start:prod": "npm run start -- prod",
    "dev": "nodemon src/index.js dev",
    "db:seed": "node scripts/run-migrations.js",
    "db:seed:dev": "npm run db:seed -- dev",
    "db:seed:prod": "npm run db:seed -- prod",
    "compose-db:generate": "node core/scripts/generate-compose.js",
    "compose-db:generate:prod": "npm run compose-db:generate -- prod",
    "compose-db:generate:dev": "npm run compose-db:generate -- dev",
    "compose-redis:generate": "node core/scripts/generate-redis-compose.js",
    "compose-redis:generate:prod": "npm run compose-redis:generate -- prod",
    "compose-redis:generate:dev": "npm run compose-redis:generate -- dev"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.18.0",
    "psf-core": "file:./core",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.2"
  }
}
